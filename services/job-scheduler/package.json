  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/bull": "^10.0.0",
    "bull": "^4.11.0",
    "cache-manager": "^5.2.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "redis": "^4.6.0",
    "cron-parser": "^4.9.0",
    "node-cron": "^3.0.2",
    "reflect-metadata": "^0.1.13",
